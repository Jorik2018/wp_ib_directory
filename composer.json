{
    "name": "10quality/wpmvc",
    "type": "project",
    "description": "WordPress MVC Framework.",
    "keywords": [
        "framework",
        "wpmvc",
        "wordpress",
        "mvc"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "10quality/wpmvc-core": "^3.1",
        "imangazaliev/didom": "^2.0"
    },
    "require-dev": {
        "10quality/wpmvc-commands": "1.1.*"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "vendor-dir": "../vendor",
        "autoloader-suffix":"wp_ib_directory"
    },
    "autoload": {
        "psr-4": {
            "IB\\directory\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
